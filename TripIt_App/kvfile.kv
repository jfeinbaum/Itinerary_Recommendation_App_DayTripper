<ScreenOne>:
	BoxLayout:
	    orientation: 'vertical'
		Button:
			text: "Recomendation"
			background_color : 0, 0, 1, 1
			size: 50, 50
			size_hint: (0.5, 0.5)
			on_press:
				# You can define the duration of the change
				# and the direction of the slide
				root.printfunction()
				root.manager.current = 'screen_two'
		Button:
			text: "Script"
			background_color : 0, 0, 1, 1
			size: 50, 50
			on_press:
				# You can define the duration of the change
				# and the direction of the slide
				root.printfunction()
				root.manager.current = 'screen_two'

